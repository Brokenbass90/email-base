*
  font-family "Roboto", "Helvetica", "Arial", sans-serif !important

.vam
  vertical-align middle !important


.butt-td
  padding: 0 !important

body
  margin: 0 auto !important
  text-align: center !important

table.body
  background: #ffffff
  
table.row
  background: #ffffff

.text-pad-small
  @media screen and (max-width: 600px)
    padding-left: 20px !important
    padding-right: 20px !important

.center
  text-align: center
  margin: 0 auto

.bb
  border-bottom: 2px solid #EAEBEE

.nopoint
  cursor: default !important

.logo
  float: none
  max-width 129px
  padding-left: 0
  padding-top: 0
  padding-bottom: 0
  @media screen and (max-width: 600px)
    padding-left: 20px !important

.header-soc
  float: right
  @media screen and (max-width: 600px)
    padding-right: 20px !important
  a
    display: inline-block
    margin-left: 20px
    @media screen and (max-width: 600px)
      margin-left: 15px !important

.ig
  max-width: 15px
  float: none
.tw
  max-width: 19px
  float: none
.yt
  max-width: 22px
  float: none
.fb
  max-width: 15px
  float: none

.title
  color: #2A3554
  font-size: 38px
  line-height: 49px
  font-weight: 700
  font-family: Helvetica !important

.text
  font-family: Helvetica !important
  color #2A3554
  font-size 16px
  font-weight 400
  line-height 24px

.text a
  color: #213E8F
  font-weight: 700
  font-family: Helvetica !important

.small-text
  color: #5C6378
  font-size: 12px
  line-height: 20px
  font-weight: 400
  font-family: Helvetica !important
  
span.red
  color: #FE4547
  font-weight: 700
  font-family: Helvetica !important

.big-text
  color: #2A3554
  font-size: 22px
  line-height: 24px
  font-weight: 700
  font-family: Helvetica !important

.center-small
  @media screen and (max-width: 600px)
    text-align: center !important

.button-wrapper
  padding-top: 0
  padding-bottom: 0
  @media screen and (max-width: 600px)
    padding-left: 30px !important
    padding-right: 30px !important

table.medium-button td
  background: #FE4547
  border-radius: 3px !important

.butt
  color #ffffff
  font-size 14px !important
  font-weight 700 !important
  border-radius: 3px !important
  text-transform: uppercase
  letter-spacing: 1px !important
  padding: 18px 0 17px !important
  font-family: Helvetica !important

.left-side
  display: inline-block
  width: 60%
  max-width: 280px
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box
  vertical-align: middle
  @media screen and (max-width: 600px)
    width: 100% !important
    max-width: 100% !important
    display: block !important

.right-side
  display: inline-block
  width: 40%
  padding-left: 20px
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box
  vertical-align: middle
  @media screen and (max-width: 600px)
    width: 100% !important
    display: block !important
    padding-left: 0 !important
    padding-top: 20px !important

.grey-block
  background: #EDF0F7
  border-radius: 5px
  padding: 20px

.block-title
  color: #2A3554
  font-size: 20px
  line-height: 26px
  font-family: Helvetica !important
  font-weight: 700

.block-text
  color: #2A3554
  font-size: 14px
  line-height: 22px
  font-weight: 400
  font-family: Helvetica !important
  a
    color: #213E8F
    font-family: Helvetica !important
    text-decoration: underline

p.warning
  text-align: left
  color: #7E859A !important
  font-size: 14px
  line-height: 21px
  font-family: Helvetica !important
  padding-bottom: 15px
  font-weight: 400

p.warning .upper
  color #525D78
  font-size 14px
  font-weight 700
  text-align: left
  text-transform: uppercase
  padding-bottom: 8px
  display: block
  letter-spacing 1px
  font-family: Helvetica !important

.address-text
  color: #7E859A !important
  font-size: 14px !important
  font-family: Helvetica !important
  font-weight: 400
  text-align: center

.subscribe
  text-align: left !important
  padding-top: 20px

.mobile-paddding
  padding: 25px 50px 50px
  @media screen and (max-width: 600px)
    padding-left: 20px !important
    padding-right: 20px !important

a.left
  color: #7E859A
  font-weight: 400
  display: inline-block
  text-align: left
  font-family: Helvetica !important
  @media screen and (max-width: 600px)
    display: block !important
    text-align: center !important
    padding-bottom: 10px !important

a.right
  color: #7E859A
  font-weight: 400
  display: inline-block
  text-align: right
  float: right
  font-family: Helvetica !important
  @media screen and (max-width: 600px)
    display: block !important
    text-align: center !important
    float: none !important
    padding-right: 0 !important


