*
  font-family "Helvetica", "Arial", sans-serif !important

body
  margin: 0 auto !important
  text-align: center !important
  width: 100% !important

table.body
  background: #F8F8FB

.transparent
  background: #F8F8FB
  
.white-bg
  background: #ffffff

.brad-top
  border-top-left-radius 15px
  border-top-right-radius 15px
  overflow hidden

.brad-bot
  border-bottom-left-radius 15px
  border-bottom-right-radius 15px
  overflow hidden

.text-pad-small
  @media screen and (max-width: 600px)
    padding-left: 20px !important
    padding-right: 20px !important

.center
  text-align: center
  margin: 0 auto

.bb
  border-bottom: 2px solid #EAEBEE

.nopoint
  cursor: default !important
  display: block

.img100
  float: none
  display: block
  width: 100%

.logo
  float: none
  max-width 168px
  height: 25px
  padding-left: 0
  padding-top: 0
  padding-bottom: 0
  margin: 0 auto

.title
  color: #0B1735
  font-size: 36px
  line-height: 44px
  font-weight: 700
  font-family: Helvetica !important
  @media screen and (max-width: 600px)
    font-size: 30px !important
    line-height: 42px !important

.try
  float: none
  color: #6939CF
  font-size: 14px
  line-height: 27px
  font-weight: 700
  font-family: Helvetica !important
  text-transform: uppercase

.arr
  float: none
  display: inline-block
  vertical-align: middle
  padding-left: 11px
  max-width: 10px

.text
  font-family: Helvetica !important
  color #272D3D
  font-size 18px
  font-weight 400
  line-height 27px
  
.subtitle
  color: #2A3554
  font-size: 28px
  line-height: 34px
  font-weight: 700
  font-family: Helvetica !important
  @media screen and (max-width: 600px)
    font-size: 25px !important
    line-height: 32px !important
    word-break: break-word !important

.purple-block
  background: #F8F5FF
  border-radius: 15px
  padding: 25px 20px 22px 30px
  box-sizing: border-box

.block-title
  color: #2A3554
  font-size: 24px
  line-height: 29px
  font-weight: 700
  font-family: Helvetica !important
  
.smaller-text
  color: #272D3D
  font-size: 16px
  line-height: 26px
  font-weight: 400
  font-family: Helvetica !important

.methods-img
  float: none
  display block
  max-width: 514px
  margin: 0 auto
  @media screen and (max-width: 600px)
    max-width: 100% !important

.squares
  display: block
  overflow: hidden

.square
  width: 170px
  padding: 20px
  box-sizing border-box
  float: left
  border-radius: 11px
  background-size: 100%
  margin-right: 5px
  @media screen and (max-width: 600px)
    float: none !important
    display: block !important
    margin: 0 auto 10px !important
  
.square1
  background: url('https://static.cdnpub.info/files/storage/public/60/a5/03f84575c0h8h6a4f1/sq1.png') left top no-repeat

.square2
  background: url('https://static.cdnpub.info/files/storage/public/60/a5/03f8493911h2g3b9i6/sq2.png') left top no-repeat

.square3
  background: url('https://static.cdnpub.info/files/storage/public/60/a5/03f84275f8c9a5d0h9/sq3.png') left top no-repeat
  margin-right: 0

.sq-text
  color: #FFFFFF
  font-size: 21px
  line-height: 25px
  font-weight: 700
  font-family: Helvetica !important

.icon
  float: none
  display: block
  width: 60px
  padding-bottom: 15px
  @media screen and (max-width: 600px)
    width: 60px !important

.line-text
  padding-left: 19px
  background: url('https://static.cdnpub.info/files/storage/public/60/7e/a43d7eca79i7d1f9e4/dot.png') left top 7px no-repeat
  background-size: 10px
  color: #2A3554
  font-size: 16px
  line-height: 24px
  font-weight: 400
  font-family: Helvetica !important
  padding-bottom: 15px

.pr20
  padding-right: 20px

.text a
  color: #3F71F5
  font-family: Helvetica !important

.button-wrapper
  padding-top: 0
  padding-bottom: 0
  @media screen and (max-width: 600px)
    padding-left: 20px !important
    padding-right: 20px !important

table.medium-button td
  background: #FB5550
  border-radius: 5px !important
  padding: 0 !important

.butt
  color #ffffff
  font-size 14px !important
  line-height 17px !important
  font-weight 700 !important
  border-radius: 5px !important
  text-transform: uppercase
  letter-spacing: 1px !important
  padding: 20px 0 18px !important
  font-family: Helvetica !important

.left-side
  display: inline-block
  width: 60%
  max-width: 280px
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box
  vertical-align: middle
  @media screen and (max-width: 600px)
    width: 100% !important
    max-width: 100% !important
    display: block !important

.right-side
  display: inline-block
  width: 40%
  padding-left: 20px
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box
  vertical-align: middle
  @media screen and (max-width: 600px)
    width: 100% !important
    display: block !important
    padding-left: 0 !important
    padding-top: 20px !important

.indies
  display: block
  overflow: hidden
  margin-bottom: 20px

.indicator
  border-radius: 11px
  width: 230px
  min-height: 250px
  padding: 107px 26px 20px
  box-sizing: border-box !important
  @media screen and (max-width: 600px)
    float: none !important
    width: 100% !important
    min-height: auto !important

.ind_rsi
  background: #F1F1F9 url('https://static.cdnpub.info/files/storage/public/60/7e/a43d042059j8h1d5c5/1.png') left 9px top 18px no-repeat
  background-size: 92px
  float: left
  @media screen and (max-width: 600px)
    margin-bottom: 20px !important

.ind_awesome
  background: #F1F1F9 url('https://static.cdnpub.info/files/storage/public/60/7e/a43d0b8496a2g8d2j7/2.png') left 9px top 18px no-repeat
  background-size: 92px
  float: right

.ind_average
  background: #F1F1F9 url('https://static.cdnpub.info/files/storage/public/60/7e/a43d1fb2e5h8a6e9b8/3.png') left 9px top 18px no-repeat
  background-size: 92px
  float: left
  @media screen and (max-width: 600px)
    margin-bottom: 20px !important

.ind_momentum
  background: #F1F1F9 url('https://static.cdnpub.info/files/storage/public/60/7e/a43d4e8989f8i1f2d1/4.png') left 9px top 18px no-repeat
  background-size: 92px
  float: right

