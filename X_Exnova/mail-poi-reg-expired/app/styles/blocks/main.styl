    padding-right: 20px !important

.center
  text-align: center
  margin: 0 auto


  border-bottom: 2px solid #EAEBEE

.bt
  border-top: 2px solid #EAEBEE

.nopoint
  cursor: default !important

.logo
  float: none
  max-width 167px
  padding-left: 0
  padding-top: 0
  padding-bottom: 0
  margin: 0 auto

.title
  color: #27282D
  font-size: 38px
  line-height: 49px
  font-weight: 700
  font-family: Helvetica !important

.text
  font-family: Helvetica !important
