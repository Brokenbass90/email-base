  font-size 16px
  font-weight 400
  line-height 24px
  letter-spacing: 0

.text a
  color: #0074CC
  font-family: Helvetica !important
  word-break: keep-all !important
  word-wrap: normal !important
  display: inline-block

.small-text
  color: #3E414B
