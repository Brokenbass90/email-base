#outlook
  a
    padding 0

body
  width 100% !important
  min-width 100%
  -webkit-text-size-adjust 100%
  -ms-text-size-adjust 100%
  margin 0
  padding 0
  &.outlook
    p
      display inline !important

.ExternalClass
  width 100%

.ExternalClass,
.ExternalClass p,
.ExternalClass span,
.ExternalClass font,
.ExternalClass td,
.ExternalClass div
  line-height 100%

#backgroundTable
  margin 0
  padding 0
  width 100% !important
  line-height 100% !important

img
  outline none
  text-decoration none
  -ms-interpolation-mode bicubic
  width auto
  max-width 100%
  float left
  clear both
  display block
  &.center
    margin 0 auto
    float none

center
  width 100%
  min-width 580px

a
  color #2ba6cb
  text-decoration none
  img
    border none

table
  border-spacing 0
  border-collapse collapse
  &.body
    height 100%
    width 100%
  &.container
    width 580px
    margin 0 auto
    text-align inherit
    table
      &.row
        display block
  &.row
    padding 0px
    width 100%
    position relative
  &.one
    width 30px
    center
      min-width 30px
    .panel
      center
        min-width 10px
  &.two
    width 80px
    center
      min-width 80px
    .panel
      center
        min-width 60px
  &.three
    width 130px
    center
      min-width 130px
    .panel
      center
        min-width 110px
  &.four
    width 180px
    center
      min-width 180px
    .panel
      center
        min-width 160px
  &.five
    width 230px
    center
      min-width 230px
    .panel
      center
        min-width 210px
  &.six
    width 280px
    center
      min-width 280px
    .panel
      center
        min-width 260px
  &.seven
    width 330px
    center
      min-width 330px
    .panel
      center
        min-width 310px
  &.eight
    width 380px
    center
      min-width 380px
    .panel
      center
        min-width 360px
  &.nine
    width 430px
    center
      min-width 430px
    .panel
      center
        min-width 410px
  &.ten
    width 480px
    center
      min-width 480px
    .panel
      center
        min-width 460px
  &.eleven
    width 530px
    center
      min-width 530px
    .panel
      center
        min-width 510px
  &.twelve
    width 580px
    center
      min-width 580px
    .panel
      center
        min-width 560px
  &.tiny-button
    td
      padding 5px 0 4px
      a
        font-size 12px
        font-weight normal
  &.small-button
    td
      padding 8px 0 7px
      a
        font-size 16px
  &.medium-button
    td
      padding 12px 0 10px
      a
        font-size 20px
  &.large-button
    td
      padding 21px 0 18px
      a
        font-size 24px
  &.radius
    td
      -webkit-border-radius 3px
      -moz-border-radius 3px
      border-radius 4px
  &.round
    td
      -webkit-border-radius 500px
      -moz-border-radius 500px
      border-radius 500px

td
  word-break normal
  border-collapse collapse !important
  &.wrapper
    padding 10px 20px 0px 0px
    position relative
  &.offset-by-one
    padding-left 50px
  &.offset-by-two
    padding-left 100px
  &.offset-by-three
    padding-left 150px
  &.offset-by-four
    padding-left 200px
  &.offset-by-five
    padding-left 250px
  &.offset-by-six
    padding-left 300px
  &.offset-by-seven
    padding-left 350px
  &.offset-by-eight
    padding-left 400px
  &.offset-by-nine
    padding-left 450px
  &.offset-by-ten
    padding-left 500px
  &.offset-by-eleven
    padding-left 550px
  &.expander
    visibility hidden
    width 0px
    padding 0 !important
  &.wrapper-logo
    padding 10px 35px 0px 35px
    position relative
  &.iq
    padding 0 !important

table, tbody, tr, td
  padding 0
  vertical-align top
  text-align left

table.columns,
table.column
  margin 0

table.columns td,
table.column td
  padding 0px 0px 10px

table.columns td.sub-columns,
table.column td.sub-columns,
table.columns td.sub-column,
table.column td.sub-column
  padding-right 10px

td.sub-column, td.sub-columns
  min-width 0px

table.row td.last,
table.container td.last
  padding-right 0px

.body .columns td.one,
.body .column td.one
  width 8.333333%

.body .columns td.two,
.body .column td.two
  width 16.666666%

.body .columns td.three,
.body .column td.three
  width 25%

.body .columns td.four,
.body .column td.four
  width 33.333333%

.body .columns td.five,
.body .column td.five
  width 41.666666%

.body .columns td.six,
.body .column td.six
  width 50%

.body .columns td.seven,
.body .column td.seven
  width 58.333333%

.body .columns td.eight,
.body .column td.eight
  width 66.666666%

.body .columns td.nine,
.body .column td.nine
  width 75%

.body .columns td.ten,
.body .column td.ten
  width 83.333333%

.body .columns td.eleven,
.body .column td.eleven
  width 91.666666%

.body .columns td.twelve,
.body .column td.twelve
  width 100%

table.center, td.center
  text-align center

h1.center,
h2.center,
h3.center,
h4.center,
h5.center,
h6.center
  text-align center

span
  &.center
    display block
    width 100%
    text-align center

.show-for-small,
.hide-for-desktop
  display none

body, table.body, h1, h2, h3, h4, h5, h6, p, td
  color #222222
  font-family "Helvetica", "Arial", sans-serif
  font-weight normal
  padding 0
  margin 0
  text-align left
  line-height 1.3

h1, h2, h3, h4, h5, h6
  word-break normal

h1
  font-size 40px

h2
  font-size 36px

h3
  font-size 32px

h4
  font-size 28px

h5
  font-size 24px

h6
  font-size 20px

body, table.body, p, td
  font-size 14px
  line-height 19px

p.lead, p.lede, p.leed
  font-size 18px
  line-height 21px

.sub-grid
  table
    width 100%
  td
    &.sub-columns
      padding-bottom 0

table.button,
table.tiny-button,
table.small-button,
table.medium-button,
table.large-button
  width 100%
  overflow hidden

table.button td,
table.tiny-button td,
table.small-button td,
table.medium-button td,
table.large-button td
  display block
  width auto !important
  text-align center
  background #ff7700
  border none !important
  //color #ffffff
  padding 8px 0

table.button td .iq,
table.tiny-button td .iq,
table.small-button td .iq,
table.medium-button td .iq,
table.large-button td .iq
  display block
  width auto !important
  text-align center
  background #ff7700
  border none !important
  //color #ffffff
  padding 8px 0

table.button td a,
table.tiny-button td a,
table.small-button td a,
table.medium-button td a,
table.large-button td a
  font-weight bold
  text-decoration none
  font-family Roboto, Helvetica, arial, sans-serif
  //color #ffffff
  font-size 16px

table.button td a .iq,
table.tiny-button td a .iq,
table.small-button td a .iq,
table.medium-button td a .iq,
table.large-button td a .iq
  font-weight bold
  text-decoration none
  font-family Roboto, Helvetica, arial, sans-serif
  //color #ffffff
  font-size 16px

@media only screen and (max-width : 600px)
  table[class="body"]
    img
      width auto !important
      height auto !important
    center
      min-width 0 !important
    tbody
      display inline-table
      width 100%
    #assets-block
      &.container
        width 300px !important
    .container
      width 95% !important
    .row
      width 100% !important
      display block !important
    .wrapper
      display block !important
      padding-right 0 !important
      padding-left 0 !important
    table
      &.columns
        td
          &.expander
            width 1px !important
    .columns
    .column
      table-layout fixed !important
      float none !important
      width 100% !important
      padding-right 0px !important
      padding-left 0px !important
      display block !important

    .wrapper.first
      .columns
      .column
        display table !important
    table
      &.columns
      &.column
        td
          width 100% !important
    .columns
    .column
      td
        &.one
          width 8.333333% !important
        &.two
          width 16.666666% !important
        &.three
          width 25% !important
        &.four
          width 33.333333% !important
        &.five
          width 41.666666% !important
        &.six
          width 50% !important
        &.seven
          width 58.333333% !important
        &.eight
          width 66.666666% !important
        &.nine
          width 75% !important
        &.ten
          width 83.333333% !important
        &.eleven
          width 91.666666% !important
        &.twelve
          width 100% !important
    td
      .offset-by-one
      .offset-by-two
      .offset-by-three
      .offset-by-four
      .offset-by-five
      .offset-by-six
      .offset-by-seven
      .offset-by-eight
      .offset-by-nine
      .offset-by-ten
      .offset-by-eleven
      .offset-by-twelve
        padding-left 0 !important
    .right-text-pad
    .text-pad-right
      padding-left 10px !important
    .left-text-pad
    .text-pad-left
      padding-right 10px !important
    .hide-for-small
    .show-for-desktop
      display none !important
    .show-for-small
    .hide-for-desktop
      display inherit !important

.preheader
  display none
  font-size 1px
  color #333333
  line-height 1px
  max-height 0px
  max-width 0px
  opacity 0
  overflow hidden

