  line-height 24px

.text a
  color: #0074CC
  font-family: Helvetica !important
  word-break: keep-all !important
  word-wrap: normal !important
  display: inline-block

.small-text
  color: #3E414B
  font-size: 14px
  line-height: 22px
  font-weight: 400
  font-family: Helvetica !important
